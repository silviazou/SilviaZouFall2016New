public class Calculate {
	public static int square(int integer){
		return integer * integer;
	}
	public static int cube(int integer){
		return integer * integer * integer;
	}
	public static double average(double numOne, double numTwo, double numThree){
		//edit this
		return (numOne + numTwo + numThree)/ 2;
	}
	public static double toDegrees(double radians){
		double pie = 3.14159;
		return (pie/radians) * (180/pie);
	}
	public static double toRadians(double angle){
		double pie = 3.14159;
		return (angle/1) * (pie/180);
	}
	public static double discriminant(double a, double b, double c){\
		//revise this
		return a(x) + b(y) = c;
	}
	public static int toImproperFrac(int wholeNum, int numerator, int denominator){
		//change
		int newNumerator = wholeNum * denominator + numerator;
		String fraction = (newNumerator +  denominator);
		return (fraction);
	}
	public static int toMixedNum(int numerator, int denominator){
		return integer * integer;
	}
	public static int foil(int intOne, int intTwo, int intThree, int intFour, String variable){
		//revise this
		return (intOne  variable + intTwo) * (intThree  variable + intFour);
	}
}
