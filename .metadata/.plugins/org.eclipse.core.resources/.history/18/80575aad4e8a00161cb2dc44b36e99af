import java.util.*;
public class MinMax {
	public static void findMaxMin(){
		int max = 1; 
		int min = 1;
		int counter = 0;
		for(int i = 0; i <= 10; i++){
			Scanner initialInput = new Scanner(System.in);
			int userInput = initialInput.nextInt();
			if(userInput> max){
				max = userInput;
			}else if(userInput < min ){
				min = userInput;
			}
			if(userInput % 2 == 0){
				System.out.println((counter += userInput) + min + max);
			}
			initialInput.close();
		}
	}
}
